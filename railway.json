{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install && cd server && npm install && cd ../client && npm install && cd .. && npm run build"
  },
  "deploy": {
    "startCommand": "cd server && NODE_ENV=production node --max-old-space-size=8192 index.js"
  },
  "cron": {
    "startCommand": "node railway-mixed-generation.js"
  },
  "variables": {
    "NODE_VERSION": "20.11.0"
  }
}